<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/SettingFragment">

    <fragment
        android:id="@+id/SettingFragment"
        android:name="th.co.opendream.vbs_recorder.fragments.settings.SettingFragment"
        android:label="@string/setting_fragment_label"
        tools:layout="@layout/fragment_setting">

        <action
            android:id="@+id/action_SettingFragment_to_QrScannerFragment"
            app:destination="@id/QrScannerFragment" />

    </fragment>


    <fragment
        android:id="@+id/QrScannerFragment"
        android:name="th.co.opendream.vbs_recorder.fragments.settings.QrScannerFragment"
        android:label="@string/qr_scanner_fragment_label"
        tools:layout="@layout/fragment_qr_scanner">
    </fragment>




</navigation>